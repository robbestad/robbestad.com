{
  "name": "robbestad",
  "version": "1.1.3",
  "description": "Robbestad.com, built with react on top of node express",
  "main": "js/app.js",
  "scripts": {
    "start": "NODE_ENV=development node index.js & gulp & watchify -o js/bundle.js -v -d .",
    "build": "NODE_ENV=production browserify . | node_modules/.bin/uglifyjs -cm   > assets/bundle.min.js",
    "test": "jest",
    "postinstall": "browserify . | node_modules/.bin/uglifyjs -cm > assets/bundle.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svenanders/robbestad.com"
  },
  "engines": {
    "node": "0.10.x"
  },
  "author": "Sven Anders Robbestad",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.2.0",
    "browserify": "~4.2.2",
    "compression": "^1.1.0",
    "envify": "~2.0.1",
    "express": "^4.9.5",
    "express-stormpath": "^0.4.5",
    "flux": "^2.0.0",
    "font-awesome": "^4.2.0",
    "jade": "^1.7.0",
    "jest-cli": "~0.1.17",
    "jquery": "^2.1.1",
    "jquery-browserify": "^1.8.1",
    "mailgun": "^0.5.0",
    "moment": "^2.8.3",
    "react": "~0.11.2",
    "react-grid": "^1.1.3",
    "react-router": "^0.8.0",
    "react-sticky": "^1.0.4",
    "react-stickydiv": "^1.0.3",
    "reactify": "~0.14.0",
    "rebound": "0.0.10",
    "resolve": "~0.6.1",
    "swig": "^1.4.2",
    "uglify-js": "~2.4.15",
    "watchify": "~0.10.2"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "envify"
    ],
    "bootstrap": {
      "exports": "bootstrap",
      "depends": {
        "jquery": "$"
      }
    }
  },
  "jest": {
    "rootDir": "./js"
  },
  "devDependencies": {
    "bootstrap": "^3.2.0",
    "browser-sync": "^1.5.2",
    "del": "^0.1.3",
    "describe": "^1.2.0",
    "gulp-autoprefixer": "^1.0.1",
    "gulp-changed": "^1.0.0",
    "gulp-concat": "^2.4.1",
    "gulp-csso": "^0.2.9",
    "gulp-imagemin": "^1.0.1",
    "gulp-jshint": "^1.8.5",
    "gulp-nodemon": "^1.0.4",
    "gulp-notify": "^1.8.0",
    "gulp-plumber": "^0.6.6",
    "gulp-sass": "^1.0.0",
    "gulp-shell": "^0.2.9",
    "jquery": "^2.1.1",
    "jsdom": "^1.0.1",
    "jshint": "^2.5.6",
    "jsxhint": "^0.4.15",
    "mocha": "^1.21.5",
    "semver": "^4.0.3",
    "should": "^4.0.4",
    "underscore": "^1.7.0",
    "zombie": "^2.0.6"
  }
}
